<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= t ".the_form_contains"%> <%= pluralize(object.errors.count, "lỗi") %>.
      <% object.errors.full_messages.each do |msg| %>
        <li><%= msg == "Name has already been taken" ? "Tên tầng đã tồn tại" : msg %></li>
      <% end %>
    </div>
    <ul>
    </ul>
  </div>
<% end %>
